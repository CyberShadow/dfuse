name "dfuse"
description "A D binding for libfuse"
authors "David Soria Parra"
copyright "Copyright (c) 2014, Facebook, Inc."
homepage "https://github.com/dlang-community/dfuse"
license "BSL-1.0"
targetType "library"

configuration "default" {
	platforms "osx" "linux"
        libs "fuse" platform="linux"
        libs "osxfuse_i32" platform="osx"
}
configuration "dfuse-osx-64" {
	platforms "osx"
        libs "osxfuse"
        versions "DARWIN_USE_64_BIT_INODE"
}
